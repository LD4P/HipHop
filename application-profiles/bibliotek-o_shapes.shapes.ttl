      sh:group bibliotek-o_shapes:MeasurementPropertyGroup ;
      sh:nodeKind sh:Literal ;
      sh:targetClass <http://measurement.bibliotek-o.org/Measurement> ;
      sh:order "3"^^xsd:decimal ;
    ] ;
    
    sh:property [
      sh:path <http://measurement.bibliotek-o.org/hasUnit> ;
      sh:description "Unit used to express the measurement" ;
      sh:group bibliotek-o_shapes:MeasurementPropertyGroup ;
      sh:name "Unit" ;
      sh:nodeKind sh:IRI ;
      sh:order "4"^^xsd:decimal ;
      sh:targetClass <http://measurement.bibliotek-o.org/Measurement> ;
    ] ;
    
    sh:property [
      sh:path <http://measurement.bibliotek-o.org/measures> ;
      sh:description "Dimension or other aspect of a resource that is measured by this Measurement. For example, a Measurement may specify the length, height, weight, file size, etc. of a resource." ;
      sh:group bibliotek-o_shapes:MeasurementPropertyGroup ;
      sh:name "Dimension or other aspect" ;
      sh:nodeKind sh:IRI ;
      sh:order "5"^^xsd:decimal ;
      sh:targetClass <http://measurement.bibliotek-o.org/Measurement> ;
    ] ;
  
   sh:property [
      sh:path <http://id.loc.gov/ontologies/bibframe/soundCharacteristic> ;
      sh:description "Technical specification relating to the encoding of sound in a resource." ;
      sh:name "Sound Characteristics" ;
      sh:group bibliotek-o_shapes:AudioInstanceCharacteristicsPropertyGroup ;
      sh:nodeKind sh:IRI ;
      sh:or (
          [
             sh:class <http://id.loc.gov/ontologies/bibframe/PlayingSpeed> ;
