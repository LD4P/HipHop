bibliotek-o_shapes:AudioWorkForm
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:label "Audio form" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:targetClass bf:Audio ;

  sh:property [
      sh:path bf:genreForm ;
      sh:class <http://www.w3.org/2004/02/skos/core#Concept> ;
      sh:group bibliotek-o_shapes:TypeGenreFormPropertyGroup ;
      sh:name "Form Genre" ;
      sh:nodeKind sh:IRI ;
      sh:order "0"^^xsd:decimal ;
      bibliotek-o_shapes:lookup bibliotek-o_shapes:LD4LookupService ;
    ] ;
    
  # The following sh:path bf:genreForm/ns0:isMemberOfMADSScheme isn't meant to be a field in the form, but rather state a restriction on the use of bf:genreForm to instances from http://id.loc.gov/authorities/genreForms. 
  
  sh:property [
      sh:path <bf:genreForm/ns0:isMemberOfMADSScheme> ;
      sh:hasValue <http://id.loc.gov/authorities/genreForms> ;
      sh:description "Values for form genre terms" ;
      sh:group bibliotek-o_shapes:TypeGenreFormPropertyGroup ;
      sh:name "LCGFT Form Genre Terms" ;
      sh:nodeKind sh:IRI ;
      skos:editorialNote "Use look up service; LCGFT is too big to load directly into an RDF editor, and more importantly we don't want to create MADSAuthorities, only link to them." ;
    ] ;
    
  sh:property [
      sh:path bib:hasPreferredTitle ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Title> ;
      sh:group bibliotek-o_shapes:TitlePropertyGroup ;
      sh:minCount 1 ;
      sh:description "For audio works there can be only one preferred title per language."
      sh:name "Preferred title" ;
      sh:nodeKind sh:IRI ;
      sh:order "0"^^xsd:decimal ;
    ] ;
  sh:property [
      sh:path <http://id.loc.gov/ontologies/bibframe/title> ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Title> ;
      sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
      sh:group bibliotek-o_shapes:TitlePropertyGroup ;
      sh:name "Other title" ;
      sh:nodeKind sh:IRI ;
      sh:order "1"^^xsd:decimal ;
    ] ;
  sh:property [
      sh:path bib:hasActivity ;
      sh:name "Agent activity or role" ;
      sh:order "0"^^xsd:decimal ;
      sh:group bibliotek-o_shapes:WorkActivityPropertyGroup ;
      tosh:editWidget swa:NestedObjectEditor ;
      sh:class bib:Activity ;
    ] ;
  
  sh:property [
      sh:path <http://purl.org/dc/terms/subject> ;
      sh:group bibliotek-o_shapes:SubjectAndCoveragePropertyGroup ;
      sh:name "Subject" ;
      sh:nodeKind sh:IRI ;
      sh:or (
          [
            sh:class <http://id.loc.gov/ontologies/bibframe/Work> ;
          ]
          [
            sh:class <http://id.loc.gov/ontologies/bibframe/Instance> ;
          ]
          [
            sh:class <http://id.loc.gov/ontologies/bibframe/Item> ;
          ]
          [
            sh:class <http://id.loc.gov/ontologies/bibframe/Temporal> ;
          ]
          [
            sh:class <http://xmlns.com/foaf/0.1/Agent> ;
          ]
          [
            sh:class <http://www.lingvoj.org/ontology/Lingvo> ;
          ]
          [
            sh:class <http://www.w3.org/2004/02/skos/core#Concept> ;
          ]
          [
            sh:class <http://www.w3.org/ns/prov#Location> ;
          ]
          [
            sh:class <http://schema.org/Event> ;
          ]
        ) ;
    ] ;
  sh:property [
      sh:path bib:covers ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Temporal> ;
      sh:description "Specifies a time period covered in the content of this resource." ;
      sh:name "Time period covered" ;
      sh:group bibliotek-o_shapes:SubjectAndCoveragePropertyGroup ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      sh:path bib:covers ;
      sh:class <http://www.w3.org/ns/prov#Location> ;
      sh:description "Specifies a location covered in the content of this resource." ;
      sh:name "Location covered" ;
      sh:group bibliotek-o_shapes:SubjectAndCoveragePropertyGroup ;
      sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
      sh:path bib:isSubjectOf ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Work> ;
      sh:description "Specifies other works that this work is the subject of" ;
      sh:name "Is subject of" ;
      sh:group bibliotek-o_shapes:SubjectAndCoveragePropertyGroup ;
      sh:nodeKind sh:IRI ;
    ] ;
    
# May decide to go with bf:eventContent to align with Performed Music decision to use BF instead of reusing schema:recordedAt. bf:eventContent domain should be open so schema:Events can be used without the added bf:Event entailment. Will need to reconcile schema and PMO class hierarchy.

    sh:property [
      sh:path <http://schema.org/recordedAt> ;
      sh:description "An event (a performance, concert, audition, etc.) wholly or partially captured by the work." ;
      sh:name "Event recorded" ;
      sh:group bibliotek-o_shapes:SubjectAndCoveragePropertyGroup ;
      sh:nodeKind sh:IRI ;
      bibliotek-o_shapes:lookup bibliotek-o_shapes:LD4LookupService ;
      skos:editorialNote "Use look up service; though FAST has some events, catalogers will have to create most events because libraries traditionally haven't created many Event authorities." ;
      
      sh:or (
          [
          sh:class <http://schema.org/Event> ;
          ] 
          [
          sh:class <http://id.loc.gov/ontologies/bibframe/Event> ;
           ]
        ) ;
    ] ;      
    
  sh:property [
      sh:path <http://rdaregistry.info/Elements/u/P60242> ;
