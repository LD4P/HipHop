          [
            sh:class <http://id.loc.gov/ontologies/bibframe/TapeConfig> ;
          ]
          [
            sh:class <http://performedmusicontology.org/ontology/DiscCutting> ;
          ]
          [
            sh:class <http:/audio.bibliotek-o.org/CaptureAndStorageTechnique> ;
