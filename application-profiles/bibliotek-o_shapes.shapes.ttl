      sh:group bibliotek-o_shapes:AudioInstanceCharacteristicsPropertyGroup ;
      sh:nodeKind sh:IRI ;
      sh:or (
          [
            sh:hasValue <http://rdaregistry.info/termList/configPlayback> 
