    <!-- Individuals -->
    
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/FPM12">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Traditional way of describing the speed of audio cylinders.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">120</rdfs:label>
        <skos:definition>Playing speed of 12 feet per minute x 10.</skos:definition>
    </owl:NamedIndividual>
    
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS.9">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed occurs on half-speed mini-cassettes used in small portable recorders.</rdfs:comment>
        <rdfs:label>15/16 ips</rdfs:label>
        <skos:definition>Playing speed of 15/16 inches per second.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS1.8">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed occurs on standard audiocassettes.</rdfs:comment>
        <rdfs:label>1 7/8 ips</rdfs:label>
        <skos:definition>Playing speed of 1 7/8 inches per second.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS15">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Occcurs on reel-to-reel, studio tapes.</rdfs:comment>
        <rdfs:label>15 ips</rdfs:label>
        <skos:definition>Playing speed of 15 inches per second.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS3.75">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed occurs on consumer and nonprofessional reel-to-reel tapes, cartridges and very unusual cassette tapes.</rdfs:comment>
        <rdfs:label>3.75 ips</rdfs:label>
        <skos:definition>Playing speed of 3.75 inches per second</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS30">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Occurs on reel-to-reel, studio tapes, but is rarely used.</rdfs:comment>
        <rdfs:label>30 ips</rdfs:label>
        <skos:definition>Playing speed of 30 inches per second.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS7.5">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed occurs in reel-to-reel, nonprofessional, and studio tapes.</rdfs:comment>
        <rdfs:label>7.5 ips</rdfs:label>
        <skos:definition>Playing speed of 7.5 inches per second.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/MPS1.4">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed is calculated from the distance the playback mechanism covers on the surface of the disc per second, and not the number of revolutions of the disc. It is associated with compact discs (CDs)</rdfs:comment>
        <rdfs:label>1.4 mps</rdfs:label>
        <skos:definition>Playing speed of 1.4 meters per second.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/RPM16">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>For audio discs</rdfs:comment>
        <rdfs:label>16 rpm</rdfs:label>
        <skos:definition>Playing speed of 16 revolutions per minute.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/RPM33">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>For audio discs.</rdfs:comment>
        <rdfs:label>33 1/3 rpm</rdfs:label>
        <skos:definition>Playing speed of 33 1/3 revolutions per minute. Often shortened to &quot;33&quot;.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/RPM45">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>For audio discs.</rdfs:comment>
        <rdfs:label>45 rpm</rdfs:label>
        <skos:definition>Playing speed of 45 revolutions per minute.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/RPM78">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>For audio discs</rdfs:comment>
        <rdfs:label>78 rpm</rdfs:label>
        <skos:definition>Playing speed of 78 revolutions per minute.</skos:definition>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/RPM8">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>For audio discs</rdfs:comment>
        <rdfs:label>8 rpm</rdfs:label>
        <skos:definition>Playing speed of 8 revolutions per minute.</skos:definition>
    </owl:NamedIndividual>

